<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
                <value id="osm" type="host">*.openstreetmap.org</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
                <value id="osm" type="host">*.openstreetmap.org</value>
            </values>
        </policy>
        <policy id="media-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
                <value id="osm" type="host">*.openstreetmap.org</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
                <value id="googleapis" type="host">fonts.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="gls" type="host">*.gls.com</value>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
                <value id="gstatic" type="host">fonts.gstatic.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="szybkapaczka" type="host">*.szybkapaczka.pl</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
